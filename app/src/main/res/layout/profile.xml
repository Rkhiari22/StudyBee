<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <ImageView
        android:id="@+id/circle"
        android:layout_width="295dp"
        android:layout_height="159dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="-140dp"
        android:layout_marginTop="135dp"
        android:layout_marginEnd="252dp"
        android:layout_marginBottom="500dp"
        android:contentDescription="green"
        android:src="@drawable/sign_in_green_circle" />


    <ImageView
        android:id="@+id/penedit"
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="3dp"
        android:layout_marginTop="185dp"
        android:src="@drawable/ic_pen" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="498dp"
        android:layout_height="586dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="-26dp"
        android:layout_marginTop="-211dp"
        android:layout_marginEnd="-28dp"
        android:contentDescription="yellow"
        android:src="@drawable/sign_in_yellow_circle" />


    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="295dp"
        android:layout_height="159dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="236dp"
        android:layout_marginTop="638dp"
        android:layout_marginEnd="-125dp"
        android:layout_marginBottom="-25dp"
        android:contentDescription="green"
        android:src="@drawable/sign_in_green_circle" />




    <ImageView
        android:id="@+id/greycircle"
        android:layout_width="269dp"
        android:layout_height="207dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="70dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="70dp"
        android:src="@drawable/sign_up_grey_circle" />

    <ImageView
        android:id="@+id/whitecircle"
        android:layout_width="185dp"
        android:layout_height="129dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginStart="115dp"
        android:layout_marginTop="90dp"
        android:layout_marginEnd="115dp"
        android:src="@drawable/profilepic" />
    <ImageView
        android:id="@+id/arrow"
        android:layout_width="56dp"
        android:layout_height="44dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="-10dp"
        android:layout_marginTop="10dp"
        android:src="@drawable/ic_arrow_left" />

    <TextView
        android:id="@+id/profile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="5dp"
        android:layout_marginEnd="180dp"
        android:textSize="22sp"
        android:textStyle="bold"
        android:textColor="@android:color/black"
        android:text="Profile" />

    <EditText
        android:id="@+id/name"
        android:layout_width="139dp"
        android:layout_height="65dp"
        android:layout_below="@+id/greycircle"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="10dp"
        android:hint="Username" />

    <TextView
        android:id="@+id/student"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/name"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="30dp"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@android:color/black"
        android:text="Student at" />

    <EditText
        android:id="@+id/establishment"
        android:layout_width="130dp"
        android:layout_height="57dp"
        android:layout_below="@+id/student"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="0dp"
        android:hint="Establishment"
        android:textColor="@color/purple_500"
        android:textSize="15sp" />

    <TextView
        android:id="@+id/field"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/establishment"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@android:color/black"
        android:text="Field of study" />

    <EditText
        android:id="@+id/fields"
        android:layout_width="130dp"
        android:layout_height="57dp"
        android:layout_below="@+id/field"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="0dp"
        android:textSize="15sp"
        android:hint="Field"
        android:textColor="@color/purple_500"/>

    <TextView
        android:id="@+id/email"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/fields"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@android:color/black"
        android:text="E-mail" />

    <EditText
        android:id="@+id/emailad"
        android:layout_width="130dp"
        android:layout_height="57dp"
        android:layout_below="@+id/email"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="0dp"
        android:textSize="15sp"
        android:hint="Email Address"
        android:textColor="@color/purple_500"/>

    <TextView
        android:id="@+id/contact"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/emailad"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:textStyle="bold"
        android:textColor="@android:color/black"
        android:text="Contact" />

    <EditText
        android:id="@+id/contactn"
        android:layout_width="130dp"
        android:layout_height="57dp"
        android:layout_below="@+id/contact"
        android:layout_alignParentStart="true"
        android:layout_marginStart="150dp"
        android:layout_marginTop="0dp"
        android:textSize="15sp"
        android:hint="Phone number"
        android:textColor="@color/purple_500"/>

    <TextView
        android:id="@+id/edit"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/penedit"
        android:layout_alignParentStart="true"
        android:layout_marginStart="10dp"
        android:layout_marginTop="-5dp"
        android:text="Edit" />

    <TextView
        android:id="@+id/logout"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginEnd="2dp"
        android:layout_marginBottom="95dp"
        android:textSize="22sp"
        android:textColor="@android:color/black"
        android:text="Log out" />

    <ImageView
        android:id="@+id/logoutic"
        android:layout_width="40dp"
        android:layout_height="50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="340dp"
        android:layout_marginTop="701dp"
        android:src="@drawable/logout" />

</RelativeLayout>